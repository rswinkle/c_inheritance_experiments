# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug_linux
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug_linux)
  c_shapes_config = debug_linux
endif
ifeq ($(config),release_linux)
  c_shapes_config = release_linux
endif

PROJECTS := c_shapes

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

c_shapes:
ifneq (,$(c_shapes_config))
	@echo "==== Building c_shapes ($(c_shapes_config)) ===="
	@${MAKE} --no-print-directory -C . -f c_shapes.make config=$(c_shapes_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f c_shapes.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug_linux"
	@echo "  release_linux"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   c_shapes"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"